    "@types/mocha": "^5.2.5",
    "@types/node": "^10.10.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.0.1",
    "sinon": "^6.3.4",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "~5.8.0",
    "typescript": "~2.8.3"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/tslamic/simple-pubsub/blob/master/README.md",
