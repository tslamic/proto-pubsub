    "require": [
      "ts-node/register"
    ],
    "sourceMap": true
  },
  "repository": "github:tslamic/simple-pubsub",
  "scripts": {
    "build": "tsc",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "tslint --project tsconfig.json",
    "test": "nyc node_modules/mocha/bin/mocha --require ts-node/register **/*.test.ts"
